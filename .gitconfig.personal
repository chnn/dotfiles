; [include]
; 	path = ./.gitconfig.personal

[core]
	pager = delta

[interactive]
	diffFilter = delta --color-only

[delta]
	navigate = true    # use n and N to move between diff sections
	side-by-side = true
	syntax-theme = base16

[merge]
	conflictstyle = zdiff3

[fetch]
	prune = true

[rerere]
	enabled = true
